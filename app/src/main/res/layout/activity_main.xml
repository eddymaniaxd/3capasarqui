<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Presentacion.MainActivity">

    <TextView
        android:id="@+id/txtcliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="206dp"
        android:text="@string/txtmenu"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgcliente"
        android:layout_width="133dp"
        android:layout_height="126dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="156dp"
        app:constraint_referenced_tags="@string/informacion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/vendedor"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imgcategoria"
        android:layout_width="149dp"
        android:layout_height="135dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="268dp"
        app:constraint_referenced_tags="@string/informacion"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/categoria"
        tools:ignore="MissingConstraints" />
